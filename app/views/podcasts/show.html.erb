<h1><%= "#{@podcast.name}'s Profile Page"%></h1>

<ul>
  <li> <%= @podcast.body %></li>
  <li> <%= @podcast.image %></li>
  <li> <%= @podcast.url %></li>
  <li> <%= @podcast.rating %></li>
</ul>

<h3> Genres </h3>
<ul>
<% @podcast.genres.each do |genre| %>
  <li><%=genre.name%></li>
  <%end%>
</ul>



